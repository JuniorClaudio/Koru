<table class="table table-hover">
  <thead>
    <tr>
      <th>DNI</th>
      <th>Formularios</th>
      <th>Acciones</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.dni %></td>
        <td><%= user.questionnaires.count %></td>
        <td>
          <%= link_to "<i class='info icon'></i>".html_safe, "#" %>
          <%= link_to "<i class='write icon'></i>".html_safe, "#" %>
          <%= link_to "<i class='remove icon'></i>".html_safe, "#" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= will_paginate @users, class: "custom-pagination" %>